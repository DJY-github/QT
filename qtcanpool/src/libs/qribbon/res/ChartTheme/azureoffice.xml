<?xml version="1.0" encoding="UTF-8"?>
<Theme>
  <Chart>
    <Border>
      <Color>#6AC0FF</Color>
      <Rounded round="5"/>
    </Border>
    <Background style="LinearGradientPattern" gradientDirection="BottomToTop">
      <Color position="0.0">#F8FCFC</Color>
      <Color position="1.0">#C7E7FF</Color>
    </Background>
    <Title>
      <Color>#0094FF</Color>
    </Title>
    <Legend>
      <TextColor>#0064FF</TextColor>
      <Background style="LinearGradientPattern" gradientDirection="BottomToTop">
        <Color position="0.0">#C7E7FF</Color>
        <Color position="1.0">#F8FCFC</Color>
      </Background>
      <Border>
        <Color>#6AC0FF</Color>
        <Rounded round="0"/>
      </Border>
    </Legend>

    <!---  -->
    <Area2D>
      <Background style="LinearGradientPattern" gradientDirection="TopToBottom">
        <Color alpha="200" position="0.0">#F8FCFC</Color>
        <Color alpha="200" position="1.0">#C7E7FF</Color>
      </Background>
      <Border>
        <Color>#6AC0FF</Color>
      </Border>
      <Axis>
        <Color>#6AC0FF</Color>
        <InterlacedColor>#F8FCFC;#F8FCFC</InterlacedColor>

        <GridLines>
          <Color>#6AC0FF</Color>
          <MinorColor>#6AC0FF</MinorColor>
        </GridLines>
        
        <AxisLabel>
          <TextColor>#0064FF</TextColor>
        </AxisLabel>
        
        <AxisTitle>
          <TextColor>#0094FF</TextColor>
        </AxisTitle>
        
        <ConstantLine>
          <Color>#4C5C2B</Color>
          <TextColor>#0094FF</TextColor>
        </ConstantLine>

        <StripColor>#F6F6F3;#FAFAF7</StripColor>
        
      </Axis>
    </Area2D>
  </Chart>
  
  <SeriesImpl>
    <Label>
      <BackgroundColor>#FFFFFF</BackgroundColor>
    </Label>
  </SeriesImpl>

  <PieSeries>
    <BorderColor>#85FFFFFF</BorderColor>
  </PieSeries>

  <SeriePalette>
    <Color>#4F81BD;#335A88</Color>
    <Color>#C0504D;#8B3431</Color>
    <Color>#9BBB59;#6F8938</Color>
    <Color>#8064A2;#594573</Color>
    <Color>#4BACC6;#2E7C91</Color>
    <Color>#F79646;#D56509</Color>
  </SeriePalette>

  </Theme>
