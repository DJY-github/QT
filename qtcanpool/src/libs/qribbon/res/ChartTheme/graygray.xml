<?xml version="1.0" encoding="UTF-8"?>
<Theme name ="GrayGray">
  <Chart>
    <Border>
      <Color>#858585</Color>
      <Rounded round="5"/>
    </Border>
    <Background style="LinearGradientPattern" gradientDirection="BottomToTop">
      <Color alpha="50" position="0.0">#E5E5E5</Color>
      <Color alpha="50" position="1.0">#000000</Color>
    </Background>
    <Title>
      <Color>#000000</Color>
    </Title>
    <Legend>
      <TextColor>#000000</TextColor>
      <Background style="SolidPattern">
        <Color alpha="20" position="0.0">#000000</Color>
      </Background>
      <Border>
        <Color>#858585</Color>
        <Rounded round="0"/>
      </Border>
    </Legend>

    <!---  -->
    <Area2D>
      <Background style="LinearGradientPattern" gradientDirection="BottomTopToTop">
        <Color alpha="20" position="0.0">#000000</Color>
        <Color alpha="20" position="1.0">#FFFFFF</Color>
      </Background>
      <Border>
        <Color>#333333</Color>
      </Border>
      <Axis>
        <Color>#333333</Color>
        <InterlacedColor>#E9E9E9;#E9E9E9</InterlacedColor>

        <GridLines>
          <Color>#858585</Color>
          <MinorColor>#F3F3F3</MinorColor>
        </GridLines>
        
        <AxisLabel>
          <TextColor>#000000</TextColor>
        </AxisLabel>
        
        <AxisTitle>
          <TextColor>#000000</TextColor>
        </AxisTitle>
        
        <ConstantLine>
          <Color>#4C5C2B</Color>
          <TextColor>#0094FF</TextColor>
        </ConstantLine>

        <StripColor>#333333;#333333</StripColor>
        
      </Axis>
    </Area2D>
  </Chart>

  <SeriesImpl>
    <Label>
      <BackgroundColor>#FFFFFF</BackgroundColor>
    </Label>
  </SeriesImpl>

  <PieSeries>
    <BorderColor>#85FFFFFF</BorderColor>
  </PieSeries>

  <SeriePalette>
    <Color>#333333;#606060</Color>
    <Color>#555555;#868686</Color>
    <Color>#666666;#939393</Color>
    <Color>#999999;#C3C3C3</Color>
    <Color>#CCCCCC;#EAEAEA</Color>
    <Color>#DDDDDD;#F3F3F3</Color>
    <Color>#E5E5E5;#FFFFFF</Color>
  </SeriePalette>

</Theme>
